{
  "name": "learning-git-2",
  "version": "1.0.0",
  "description": "https://patryk-rozwadowski.github.io/learning-git-2/",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdir sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:sass": "node_modules/.bin/node-sass --output-style compact -o style.css style.scss",
    "build:autoprefixer": "autoprefixer-cli stylesheets/style.css",
    "build-dev:sass": "node_modules/.bin/node-sass --output-style expanded --source-map true -o css sass",
    "watch:sass": "node_modules/.bin/node-sass --output-style expanded --source-map true -o css sass --watch",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o stylesheets/style.prefixed.css stylesheets/style.css",
    "watch": "npm-run-all build -p watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Patryk-Rozwadowski/learning-git-2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Patryk-Rozwadowski/learning-git-2/issues"
  },
  "homepage": "https://github.com/Patryk-Rozwadowski/learning-git-2#readme",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0"
  }
}
